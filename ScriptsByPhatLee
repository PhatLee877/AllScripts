loadstring(game:HttpGet("https://pastefy.app/pg2sgGIu/raw"))()

local CoreGui = game:GetService("CoreGui")

-- Kill it if it already exists
local trigger = CoreGui:FindFirstChild("TriggerPoint", true)
if trigger then
    trigger:Destroy()
end

-- Kill it instantly whenever it's added
CoreGui.DescendantAdded:Connect(function(obj)
    if obj.Name == "TriggerPoint" then
        obj:Destroy()
    end
end)

-- ðŸ“Œ Auto copy TikTok link
setclipboard("https://www.tiktok.com/@developervn11?_t=ZS-8yW9cZdXeQD&_r=1")

-- ðŸ“Œ Load Rayfield UI Library
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()

-- ðŸ“Œ Create main GUI window
local Window = Rayfield:CreateWindow({
    Name = "All Script By PhatLee(and more)",
    Icon = 0,
    LoadingTitle = "Follow Tiktok Scripts By PhatLee For All Update",
    LoadingSubtitle = "By Phat Lee",
    ShowText = "Rayfield",
    Theme = "Default",
    ToggleUIKeybind = "K",
    DisableRayfieldPrompts = false,
    DisableBuildWarnings = false,
    ConfigurationSaving = {
        Enabled = true,
        FolderName = nil,
        FileName = "PhatLeeScriptConfig"
    },
    Discord = {
        Enabled = false,
        Invite = "",
        RememberJoins = false
    },
    KeySystem = false
})

